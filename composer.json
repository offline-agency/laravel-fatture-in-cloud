{
  "name": "offline-agency/laravel-fatture-in-cloud",
  "description": "An integration plugin with Fatture in Cloud Api written in Laravel PHP",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "integration",
    "api",
    "fatture in cloud"
  ],
  "authors": [
    {
      "name": "Offline Agency",
      "email": "info@offlineagency.it"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "ext-json": "*",
    "illuminate/support": "6.0|^7.0|^8.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "illuminate/collections": "6.0|^7.0|^8.0",
    "illuminate/container": "6.0|^7.0|^8.0",
    "illuminate/http": "6.0|^7.0|^8.0"
  },
  "require-dev": {
    "doctrine/dbal": "^2.5",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0|^6.0",
    "php-coveralls/php-coveralls": "^2.4",
    "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "OfflineAgency\\FattureInCloud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": []
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "OfflineAgency\\FattureInCloud\\FattureInCloudServiceProvider"
      ],
      "aliases": {
        "FattureInCloud": "OfflineAgency\\FattureInCloud\\FattureInCloudFacade"
      }
    }
  }
}
